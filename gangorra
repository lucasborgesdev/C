#include <stdio.h>
#include <stdlib.h>

int main()
{
int p1, c1, p2, c2, s1, s2;
scanf("%d", &p1);
scanf("%d", &c1);
scanf("%d", &p2);
scanf("%d", &c2);
if((p1 * c1) == (p2 * c2)){
    printf("0");
    }

    else{
        if((p1 * c1) > (p2 * c2)){
        printf("-1");
        }
        else{
            printf("1");
        }


}
return 0;

}


